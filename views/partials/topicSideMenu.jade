.tabbed-side-menu-component.tb
  h3 {{current.topic.name}}
  tabs
    tab(data-tab-id='feed', caption='Topics')
      ul
        li(ng-repeat='(id, topic) in user.topics | orderBy:"name"')
          a(href='#/topics/{{id}}') {{topic.name}}

    tab(data-tab-id='topics', caption='Feed WIP')
      ul
        li(ng-repeat='(topic, id) in user.topics | orderBy:"name"')
          a(href='{{topic.url}}') asd {{topic.name}}