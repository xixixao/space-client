.pdf-viewer-control * {
  padding: 0;
  margin: 0;
}

mainTopOffset = 40px
toolbarHeight = 32px
totalTopOffset = calc(mainTopOffset + toolbarHeight)

/* These are parent to layout */


/* END */


.pdf-viewer-control {
  background-color: #404040;
  background-image: url('/img/viewer/texture.png');
}

.pdf-viewer-control,
.pdf-viewer-control input,
.pdf-viewer-control button,
.pdf-viewer-control select {
  font: message-box;
}

.pdf-hidden {
  display: none !important;
}

[hidden] {
  display: none !important;
}

#pdf-viewerContainer:-webkit-full-screen {
  top: 0px;
  border-top: 2px solid transparent;
  background-color: #404040;
  background-image: url('/img/viewer/texture.png');
  width: 100%;
  height: 100%;
  overflow: hidden;
  cursor: none;
}

#pdf-viewerContainer:-moz-full-screen {
  top: 0px;
  border-top: 2px solid transparent;
  background-color: #404040;
  background-image: url('/img/viewer/texture.png');
  width: 100%;
  height: 100%;
  overflow: hidden;
  cursor: none;
}

#pdf-viewerContainer:fullscreen {
  top: 0px;
  border-top: 2px solid transparent;
  background-color: #404040;
  background-image: url('/img/viewer/texture.png');
  width: 100%;
  height: 100%;
  overflow: hidden;
  cursor: none;
}


:-webkit-full-screen .pdf-page {
  margin-bottom: 100%;
}

:-moz-full-screen .pdf-page {
  margin-bottom: 100%;
}

:fullscreen .pdf-page {
  margin-bottom: 100%;
}

:-webkit-full-screen a:not(.internalLink) {
  display: none;
}

:-moz-full-screen a:not(.internalLink) {
  display: none;
}

:fullscreen a:not(.internalLink) {
  display: none;
}

#pdf-viewerContainer.pdf-presentationControls {
  cursor: default;
}

/* outer/inner center provides horizontal center */
html[dir='ltr'] .pdf-outerCenter {
  float: right;
  position: relative;
  right: 50%;
}
html[dir='rtl'] .pdf-outerCenter {
  float: left;
  position: relative;
  left: 50%;
}
html[dir='ltr'] .pdf-innerCenter {
  float: right;
  position: relative;
  right: -50%;
}
html[dir='rtl'] .pdf-innerCenter {
  float: left;
  position: relative;
  left: -50%;
}

#pdf-outerContainer {
  width: 100%;
  height: 100%;
}

#pdf-sidebarContainer {
  position: absolute;
  top: mainTopOffset;
  bottom: 0;
  width: 200px;
  visibility: hidden;
  -webkit-transition-duration: 200ms;
  -webkit-transition-timing-function: ease;
  -moz-transition-duration: 200ms;
  -moz-transition-timing-function: ease;
  -ms-transition-duration: 200ms;
  -ms-transition-timing-function: ease;
  -o-transition-duration: 200ms;
  -o-transition-timing-function: ease;
  transition-duration: 200ms;
  transition-timing-function: ease;

}
html[dir='ltr'] #pdf-sidebarContainer {
  -webkit-transition-property: left;
  -moz-transition-property: left;
  -ms-transition-property: left;
  -o-transition-property: left;
  transition-property: left;
  left: -200px;
}
html[dir='rtl'] #pdf-sidebarContainer {
  -webkit-transition-property: right;
  -ms-transition-property: right;
  -o-transition-property: right;
  transition-property: right;
  right: -200px;
}

#pdf-outerContainer.sidebarMoving > #pdf-sidebarContainer,
#pdf-outerContainer.pdf-sidebarOpen > #pdf-sidebarContainer {
  visibility: visible;
}
html[dir='ltr'] #pdf-outerContainer.pdf-sidebarOpen > #pdf-sidebarContainer {
  left: 0px;
}
html[dir='rtl'] #pdf-outerContainer.pdf-sidebarOpen > #pdf-sidebarContainer {
  right: 0px;
}

#pdf-mainContainer {
  position: absolute;
  top: mainTopOffset;
  right: 0;
  bottom: 0;
  left: 0;
  min-width: 320px;
  -webkit-transition-duration: 200ms;
  -webkit-transition-timing-function: ease;
  -moz-transition-duration: 200ms;
  -moz-transition-timing-function: ease;
  -ms-transition-duration: 200ms;
  -ms-transition-timing-function: ease;
  -o-transition-duration: 200ms;
  -o-transition-timing-function: ease;
  transition-duration: 200ms;
  transition-timing-function: ease;
}
html[dir='ltr'] #pdf-outerContainer.pdf-sidebarOpen > #pdf-mainContainer {
  -webkit-transition-property: left;
  -moz-transition-property: left;
  -ms-transition-property: left;
  -o-transition-property: left;
  transition-property: left;
  left: 200px;
}
html[dir='rtl'] #pdf-outerContainer.pdf-sidebarOpen > #pdf-mainContainer {
  -webkit-transition-property: right;
  -moz-transition-property: right;
  -ms-transition-property: right;
  -o-transition-property: right;
  transition-property: right;
  right: 200px;
}

#pdf-sidebarContent {
  top: toolbarHeight;
  bottom: 0;
  overflow: auto;
  position: absolute;
  width: 200px;

  background-color: hsla(0,0%,0%,.1);
  box-shadow: inset -1px 0 0 hsla(0,0%,0%,.25);
}
html[dir='ltr'] #pdf-sidebarContent {
  left: 0;
}
html[dir='rtl'] #pdf-sidebarContent {
  right: 0;
}

#pdf-viewerContainer {
  overflow: auto;
  box-shadow: inset 1px 0 0 hsla(0,0%,100%,.05);
  position: absolute;
  top: toolbarHeight;
  right: 0;
  bottom: 0;
  left: 0;
}
.pdf-loadingInProgress #pdf-viewerContainer {
  top: 39px;
}

.pdf-toolbar {
  position: relative;
  left: 0;
  right: 0;
  z-index: 9999;
  cursor: default;
}

#pdf-toolbarContainer {
  width: 100%;
}

#pdf-toolbarSidebar {
  width: 200px;
  height: 32px;
  background-color: #424242; /* fallback */
  background-image: url('/img/viewer/texture.png'),
                    linear-gradient(hsla(0,0%,30%,.99), hsla(0,0%,25%,.95));
  box-shadow: inset -1px 0 0 rgba(0, 0, 0, 0.25),
              inset 0 -1px 0 hsla(0,0%,100%,.05),
              0 1px 0 hsla(0,0%,0%,.15),
              0 0 1px hsla(0,0%,0%,.1);
}

#pdf-toolbarContainer, .pdf-findbar, .pdf-secondaryToolbar {
  position: relative;
  height: 32px;
  background-color: #474747; /* fallback */
  background-image: url('/img/viewer/texture.png'),
                    linear-gradient(hsla(0,0%,32%,.99), hsla(0,0%,27%,.95));
  box-shadow: inset 1px 0 0 hsla(0,0%,100%,.08),
              inset 0 1px 1px hsla(0,0%,0%,.15),
              inset 0 -1px 0 hsla(0,0%,100%,.05),
              0 1px 0 hsla(0,0%,0%,.15),
              0 1px 1px hsla(0,0%,0%,.1);
}

#pdf-toolbarViewer {
  height: 32px;
}

#pdf-loadingBar {
  position: relative;
  width: 100%;
  height: 6px;
  background-color: #333;
  border-bottom: 1px solid #333;
}

#pdf-loadingBar .pdf-progress {
  position: absolute;
  top: mainTopOffset;
  left: 0;
  width: 0%;
  height: 100%;
  background-color: #ddd;
  overflow: hidden;
  -moz-transition: width 200ms;
  -ms-transition: width 200ms;
  -webkit-transition: width 200ms;
  transition: width 200ms;
}

@-moz-keyframes progressIndeterminate {
  0% { left: 0%; }
  50% { left: 100%; }
  100% { left: 100%; }
}

@-ms-keyframes progressIndeterminate {
  0% { left: 0%; }
  50% { left: 100%; }
  100% { left: 100%; }
}

@-webkit-keyframes progressIndeterminate {
  0% { left: 0%; }
  50% { left: 100%; }
  100% { left: 100%; }
}

@keyframes progressIndeterminate {
  0% { left: 0%; }
  50% { left: 100%; }
  100% { left: 100%; }
}

#pdf-loadingBar .pdf-progress.pdf-indeterminate {
  background-color: #999;
  -moz-transition: none;
  -ms-transition: none;
  -webkit-transition: none;
  transition: none;
}

#pdf-loadingBar .pdf-indeterminate .pdf-glimmer {
  position: absolute;
  top: mainTopOffset;
  left: 0;
  height: 100%;
  width: 50px;

  background-image: linear-gradient(to right, #999 0%, #fff 50%, #999 100%);
  background-size: 100% 100% no-repeat;

  -moz-animation: progressIndeterminate 2s linear infinite;
  -ms-animation: progressIndeterminate 2s linear infinite;
  -webkit-animation: progressIndeterminate 2s linear infinite;
  animation: progressIndeterminate 2s linear infinite;
}

.pdf-findbar, .pdf-secondaryToolbar {
  top: totalTopOffset;
  position: absolute;
  z-index: 10000;
  height: 32px;

  min-width: 16px;
  padding: 0px 6px 0px 6px;
  margin: 4px 2px 4px 2px;
  color: hsl(0,0%,85%);
  font-size: 12px;
  line-height: 14px;
  text-align: left;
  cursor: default;
}

html[dir='ltr'] .pdf-findbar {
  left: 68px;
}

html[dir='rtl'] .pdf-findbar {
  right: 68px;
}

.pdf-findbar label {
  -webkit-user-select: none;
  -moz-user-select: none;
}

#pdf-findInput[data-status="pending"] {
  background-image: url('/img/viewer/loading-small.png');
  background-repeat: no-repeat;
  background-position: right;
}

.pdf-secondaryToolbar {
  display: none;
  padding: 6px 10px;
  width: auto;
  max-width: 220px;
  height: auto;
  z-index: 20000;
}

.pdf-secondaryToolbar > #pdf-secondaryToolbarButtonContainer {
  height: auto;
  max-height: 440px;
  overflow-y: auto;
}

#pdf-outerContainer.pdf-secondaryToolbarOpen > #pdf-secondaryToolbar {
  display: block;
}
html[dir='ltr'] #pdf-outerContainer.pdf-secondaryToolbarOpen > #pdf-secondaryToolbar {
  right: 4px;
}
html[dir='rtl'] #pdf-outerContainer.pdf-secondaryToolbarOpen > #pdf-secondaryToolbar {
  left: 4px;
}

.pdf-doorHanger,
.pdf-doorHangerRight {
  border: 1px solid hsla(0,0%,0%,.5);
  border-radius: 2px;
  box-shadow: 0 1px 4px rgba(0, 0, 0, 0.3);
}
.pdf-doorHanger:after, .pdf-doorHanger:before,
.pdf-doorHangerRight:after, .pdf-doorHangerRight:before {
  bottom: 100%;
  border: solid transparent;
  content: " ";
  height: 0;
  width: 0;
  position: absolute;
  pointer-events: none;
}
.pdf-doorHanger:after,
.pdf-doorHangerRight:after {
  border-bottom-color: hsla(0,0%,32%,.99);
  border-width: 8px;
}
.pdf-doorHanger:before,
.pdf-doorHangerRight:before {
  border-bottom-color: hsla(0,0%,0%,.5);
  border-width: 9px;
}

html[dir='ltr'] .pdf-doorHanger:after,
html[dir='rtl'] .pdf-doorHangerRight:after {
  left: 13px;
  margin-left: -8px;
}

html[dir='ltr'] .pdf-doorHanger:before,
html[dir='rtl'] .pdf-doorHangerRight:before {
  left: 13px;
  margin-left: -9px;
}

html[dir='rtl'] .pdf-doorHanger:after,
html[dir='ltr'] .pdf-doorHangerRight:after {
  right: 13px;
  margin-right: -8px;
}

html[dir='rtl'] .pdf-doorHanger:before,
html[dir='ltr'] .pdf-doorHangerRight:before {
  right: 13px;
  margin-right: -9px;
}

