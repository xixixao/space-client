  .pdf-visibleSmallView {
    display: inherit;
  }
  html[dir='ltr'] #pdf-outerContainer.sidebarMoving .pdf-outerCenter,
  html[dir='ltr'] #pdf-outerContainer.sidebarOpen .pdf-outerCenter,
  html[dir='ltr'] .pdf-outerCenter {
    left: 156px;
  }
  html[dir='rtl'] #pdf-outerContainer.sidebarMoving .pdf-outerCenter,
  html[dir='rtl'] #pdf-outerContainer.sidebarOpen .pdf-outerCenter,
  html[dir='rtr'] #pdf-outerContainer .pdf-outerCenter {
    right: 156px;
  }
  .pdf-toolbarButtonSpacer {
    width: 0;
  }
}

@media all and (max-width: 510px) {
  #pdf-scaleSelectContainer, #pdf-pageNumberLabel {
    display: none;
  }
}

